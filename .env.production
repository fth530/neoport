# Production Environment Variables
NODE_ENV=production
PORT=3000

# API Keys (Production)
FINNHUB_API_KEY=your_production_api_key_here
COINGECKO_API_KEY=your_production_api_key_here

# Security
SESSION_SECRET=your_super_secure_session_secret_here

# Database
DB_PATH=/usr/src/app/portfolio.db

# Logging
LOG_LEVEL=info
LOG_FILE=/usr/src/app/logs/app.log

# Performance
CACHE_TTL=30000
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100